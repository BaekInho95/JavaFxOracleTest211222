<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label layoutX="73.0" layoutY="100.0" text="ID" />
      <Label layoutX="73.0" layoutY="135.0" text="PASSWORD" />
      <TextField fx:id="txtUserName" layoutX="167.0" layoutY="96.0" />
      <TextField fx:id="txtPassword" layoutX="167.0" layoutY="131.0" />
      <Button fx:id="btnLogin" layoutX="115.0" layoutY="197.0" mnemonicParsing="false" onAction="#login" text="로그인" />
      <Button fx:id="btnCancel" layoutX="196.0" layoutY="197.0" mnemonicParsing="false" onAction="#cancel" text="취소" />
   </children>
</AnchorPane>
